<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/purple_200">

    <ImageButton
        android:id="@+id/homescreen_checkbox_panel_cancel"
        android:layout_width="@dimen/homescreen_checkbox_panel_icon_size"
        android:layout_height="@dimen/homescreen_checkbox_panel_icon_size"
        android:layout_marginStart="16dp"
        android:background="@drawable/bottomnav_circle_ripple"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@+id/homescreen_checkbox_panel_select"
        app:layout_constraintStart_toStartOf="0"
        app:layout_constraintTop_toTopOf="@+id/homescreen_checkbox_panel_select"
        app:srcCompat="@drawable/homescreen_checkbox_panel_cancel" />

    <ImageButton
        android:id="@+id/homescreen_checkbox_panel_select"
        android:layout_width="@dimen/homescreen_checkbox_panel_icon_size"
        android:layout_height="@dimen/homescreen_checkbox_panel_icon_size"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/bottomnav_circle_ripple"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="0"
        app:layout_constraintEnd_toStartOf="@+id/homescreen_checkbox_panel_more"
        app:layout_constraintHorizontal_bias="0.18"
        app:layout_constraintStart_toEndOf="@+id/homescreen_checkbox_panel_cancel"
        app:layout_constraintTop_toTopOf="0"
        app:srcCompat="@drawable/homescreen_checkbox_panel_select_all" />

    <ImageButton
        android:id="@+id/homescreen_checkbox_panel_more"
        android:layout_width="@dimen/homescreen_checkbox_panel_icon_size"
        android:layout_height="@dimen/homescreen_checkbox_panel_icon_size"
        android:layout_marginEnd="16dp"
        android:background="@drawable/bottomnav_circle_ripple"
        android:scaleType="fitXY"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/homescreen_checkbox_panel_select"
        app:layout_constraintEnd_toEndOf="0"
        app:layout_constraintTop_toTopOf="@+id/homescreen_checkbox_panel_select"
        app:srcCompat="@drawable/homescreen_checkbox_panel_more_horiz" />

    <ImageButton
        android:id="@+id/homescreen_checkbox_panel_share"
        android:layout_width="@dimen/homescreen_checkbox_panel_icon_size"
        android:layout_height="@dimen/homescreen_checkbox_panel_icon_size"
        android:background="@drawable/bottomnav_circle_ripple"
        android:scaleType="fitXY"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="0"
        app:layout_constraintEnd_toStartOf="@+id/homescreen_checkbox_panel_more"
        app:layout_constraintHorizontal_bias="0.82"
        app:layout_constraintStart_toEndOf="@+id/homescreen_checkbox_panel_cancel"
        app:layout_constraintTop_toTopOf="0"
        app:srcCompat="@drawable/homescreen_checkbox_panel_ios_share" />

    <ImageButton
        android:id="@+id/homescreen_checkbox_panel_delete"
        android:layout_width="@dimen/homescreen_checkbox_panel_icon_size"
        android:layout_height="@dimen/homescreen_checkbox_panel_icon_size"
        android:layout_marginEnd="16dp"
        android:background="@drawable/bottomnav_circle_ripple"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@+id/homescreen_checkbox_panel_select"
        app:layout_constraintEnd_toEndOf="0"
        app:layout_constraintTop_toTopOf="@+id/homescreen_checkbox_panel_select"
        app:srcCompat="@drawable/ssfoldscreen_delete" />
</androidx.constraintlayout.widget.ConstraintLayout>
